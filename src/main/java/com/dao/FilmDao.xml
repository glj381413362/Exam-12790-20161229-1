		</selectKey>
		insert into film
		<trim prefix="(" suffix=")" suffixOverrides=",">
			<if test="filmId != null">
				film_id,
			</if>
			<if test="title != null">
				title,
			</if>
			<if test="description != null">
				description,
			</if>
			<if test="releaseYear != null">
				release_year,
